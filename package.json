{
  "name": "larios-income-tax",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host 0.0.0.0",
    "start:en": "ng serve --host 0.0.0.0 --configuration en-US",
    "start:es": "ng serve --host 0.0.0.0 --configuration es-MX",
    "build": "ng build",
    "build:i18n": "ng build --configuration production --localize",
    "build:en": "ng build --configuration en-US",
    "build:es": "ng build --configuration es-MX",
    "extract-i18n": "ng extract-i18n --output-path src/locale",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint:md": "markdownlint-cli2 \"**/*.md\" \"#node_modules\" \"#dist\" \"#.angular\"",
    "lint:md:fix": "markdownlint-cli2 --fix \"**/*.md\" \"#node_modules\" \"#dist\" \"#.angular\""
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,css,scss,json,yaml,yml}": [
      "prettier --write"
    ],
    "*.md": [
      "markdownlint-cli2 --fix",
      "prettier --write"
    ],
    "*.html": [
      "prettier --write"
    ]
  },
  "private": true,
  "packageManager": "npm@10.9.2",
  "dependencies": {
    "@angular/animations": "^21.1.2",
    "@angular/cdk": "^21.1.1",
    "@angular/common": "^21.1.2",
    "@angular/compiler": "^21.1.2",
    "@angular/core": "^21.1.2",
    "@angular/forms": "^21.1.2",
    "@angular/material": "^21.1.1",
    "@angular/platform-browser": "^21.1.2",
    "@angular/router": "^21.1.2",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "^0.16.0"
  },
  "devDependencies": {
    "@angular/build": "^21.1.2",
    "@angular/cli": "^21.1.2",
    "@angular/compiler-cli": "^21.1.2",
    "@angular/localize": "^21.1.2",
    "jsdom": "^27.1.0",
    "lint-staged": "^16.2.7",
    "markdownlint-cli2": "^0.20.0",
    "prettier": "^3.1.0",
    "typescript": "~5.9.2",
    "vitest": "^4.0.8"
  }
}
